<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    <style name="TitleBarNightStyle" tools:ignore="ResourceName">
        <item name="android:background">#FF000000</item>
        <item name="icon_left">@mipmap/bar_icon_back_white</item>
        <item name="color_left">#CCFFFFFF</item>
        <item name="color_title">#EEFFFFFF</item>
        <item name="color_right">#99FFFFFF</item>
        <item name="color_line">#FFFFFFFF</item>
        <item name="background_left">@drawable/bar_selector_selectable_black</item>
        <item name="background_right">@drawable/bar_selector_selectable_black</item>
    </style>

    <style name="test" tools:ignore="ResourceName">

